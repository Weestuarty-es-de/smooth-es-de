            <metadataFontSize>0.035</metadataFontSize>
            <gridScale>1.2</gridScale>
        </variables>
    </fontSize>
    <fontSize name="large">
        <variables>
            <gameCounterFontSize>0.0275</gameCounterFontSize>
            <systemLabelFontSize>0.038</systemLabelFontSize>
            <textlistFontSize>0.042</textlistFontSize>
            <textlistSelector>0.06</textlistSelector>
            <metadataFontSize>0.042</metadataFontSize>
            <gridScale>1.15</gridScale>
        </variables>
    </fontSize>
    <fontSize name="x-large">
